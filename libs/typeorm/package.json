{
  "name": "@shgpu-table/typeorm",
  "type": "commonjs",
  "exports": {
    ".": "./dist/typeorm/src/index.js",
    "./entities/*": "./dist/typeorm/src/entities/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.ts"
      ]
    }
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "deploy": "pnpm ts-node --transpileOnly ./runMigrations.ts",
    "migration:generate": "pnpm ts-node --transpileOnly ./generateMigration.ts",
    "setup": "pnpm run deploy && pnpm run seed",
    "seed": "pnpm ts-node --transpileOnly ./seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/pg": "^8.6.6",
    "dotenv": "^16.0.3"
  },
  "dependencies": {
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.11",
    "typescript": "^4.9.4"
  }
}

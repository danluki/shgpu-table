{
  "name": "shgpu-table",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo run dev --filter=./apps/**",
    "build:libs": "turbo run build --filter=./libs/**",
    "start:admin": "pnpm --filter=@shgpu-table/admin start",
    "start:gateway": "pnpm --filter=@shgpu-table/gateway start",
    "start:tbot": "pnpm --filter=@shgpu-table/telegram-bot start",
    "front:dev": "pnpm run --filter=@shgpu-table/frontend dev",
    "parser:dev": "pnpm run --filter=@shgpu-table/parser dev",
    "gateway:dev": "pnpm run --filter=@shgpu-table/gateway dev"
  },
  "author": "Danluki <danluki02@yandex.ru>",
  "workspaces": [
    "apps/**",
    "libs/**"
  ],
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "turbo": "^1.6.3"
  }
}